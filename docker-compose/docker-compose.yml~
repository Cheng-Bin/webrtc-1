kms6:
  build: ./kms
  restart: always
  net: "host"
  container_name: kms6
kurento-agent:
  build: ./agent
  restart: always
  container_name: kurento-agent
  net: "host"
  volumes:
    - ./conf:/webrtc/kurento-agent/config/
  volumes_from:
    - kms6
upload-speed-tester:
  build: ./speed-tester
  restart: always
  net: "host"
  container_name: upload-speed-tester
angular-webpage:
  build: ./angular
  restart: always
  net: "host"
  container_name: angular-webpage
  volumes:
    - ./conf:/conf/
